{
  "name": "ionic2-boilerplate",
  "version": "0.12.0",
  "description": "An Ionic 2 Starter kit featuring Ionic, Karma, Protractor, Jasmine, CI, and much more",
  "keywords": [
    "ionic2",
    "angular2",
    "roolup",
    "typescript",
    "karma",
    "protractor",
    "istanbul",
    "gitlab CI"
  ],
  "author": "Marco Turi <marco.turi@hotmail.it>",
  "homepage": "https://github.com/marcoturi/ionic2-boilerplate",
  "license": "MIT",
  "scripts": {
    "dev": "bnr dev",
    "build": "bnr build",
    "build-dev": "bnr build-dev",
    "test": "bnr test",
    "test:watch": "bnr test-watch",
    "pree2e": "cd www && python -m SimpleHTTPServer 8090 >> ../python_serve.log 2>&1 &",
    "e2e": "protractor",
    "pree2e:interactive": "npm run pree2e",
    "e2e:interactive": "protractor --elementExplorer",
    "ionic:serve": "ionic-app-scripts serve",
    "ionic:build": "ionic-app-scripts build",
    "ios:dev": "bnr ios:dev",
    "ios:release": "bnr ios:release",
    "ios:release:ci": "bnr ios:release:ci",
    "android:dev": "bnr android:dev",
    "android:release": "bnr android:release",
    "commit": "git-cz",
    "docs": "typedoc --options typedoc.json --exclude '**/*+(e2e|spec|index).ts' ./src/",
    "release": "standard-version --no-verify",
    "push": "git push --follow-tags origin master",
    "lint": "tslint \"src/**/*.ts\" --exclude=src/**/*.d.ts",
    "scss-lint": "scss-lint",
    "outdated": "npm outdated --depth 0",
    "post-install": "webdriver-manager update"
  },
  "betterScripts": {
    "dev": {
      "command": "ionic serve",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "web"
      }
    },
    "build": {
      "command": "ionic-app-scripts build",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-dev": {
      "command": "ionic-app-scripts build --dev",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test-watch": {
      "command": "karma start --auto-watch --no-single-run",
      "env": {
        "NODE_ENV": "test",
        "NO_COVERAGE": true
      }
    },
    "ios:dev": {
      "command": "ionic run ios --livereload",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "ios"
      }
    },
    "ios:release": {
      "command": "ionic build ios",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "ios"
      }
    },
    "ios:release:ci": {
      "command": "npm run build",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "ios"
      }
    },
    "android:dev": {
      "command": "ionic run android --livereload",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "android"
      }
    },
    "android:release": {
      "command": "ionic build android",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "android"
      }
    }
  },
  "dependencies": {
    "@angular/common": "2.1.1",
    "@angular/compiler": "2.1.1",
    "@angular/compiler-cli": "2.1.1",
    "@angular/core": "2.1.1",
    "@angular/forms": "2.1.1",
    "@angular/http": "2.1.1",
    "@angular/platform-browser": "2.1.1",
    "@angular/platform-browser-dynamic": "2.1.1",
    "@angular/platform-server": "2.1.1",
    "@ionic/storage": "1.1.6",
    "ionic-angular": "2.0.0-rc.2",
    "ionicons": "3.0.0",
    "ionic-native": "^2.2.6",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.21",
    "lodash-es": "^4.17.2"
  },
  "devDependencies": {
    "@ionic/app-scripts": "0.0.44",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.48",
    "awesome-typescript-loader": "^2.2.1",
    "better-npm-run": "0.0.13",
    "codelyzer": "1.0.0-beta.3",
    "commitizen": "^2.8.6",
    "css-loader": "^0.25.0",
    "cz-conventional-changelog": "^1.2.0",
    "ghooks": "^1.3.2",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-spec-reporter": "^2.7.0",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-coverage": "^0.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.0",
    "phantomjs-prebuilt": "^2.1.13",
    "protractor": "^4.0.11",
    "protractor-jasmine2-screenshot-reporter": "^0.3.2",
    "raw-loader": "0.5.1",
    "source-map-loader": "^0.1.5",
    "standard-version": "^3.0.0",
    "to-string-loader": "^1.1.4",
    "ts-helpers": "^1.1.1",
    "ts-node": "^1.7.0",
    "tslint-loader": "^2.1.3",
    "typedoc": "^0.5.1",
    "typescript": "^2.0.10",
    "validate-commit-msg": "^2.8.0",
    "webpack": "2.1.0-beta.25",
    "xml2js": "^0.4.17"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint && npm run scss-lint",
      "pre-push": "npm test",
      "commit-msg": "validate-commit-msg"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ionic_watch": "./config/watch.config.js",
    "ionic_webpack": "./config/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcoturi/ionic2-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/marcoturi/ionic2-boilerplate/issues"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3"
  }
}
