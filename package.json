{
  "name": "ionic2-boilerplate",
  "version": "0.27.0",
  "description": "An Ionic Starter kit featuring Ionic, Karma, Protractor, Jasmine, CI, and much more",
  "keywords": [
    "ionic2",
    "angular2",
    "roolup",
    "webpack",
    "yarn",
    "typescript",
    "karma",
    "protractor",
    "istanbul",
    "gitlab CI"
  ],
  "author": "Marco Turi <marco.turi@hotmail.it>",
  "homepage": "https://github.com/marcoturi/ionic2-boilerplate",
  "license": "MIT",
  "scripts": {
    "dev": "bnr dev",
    "build": "bnr build",
    "build-dev": "bnr build-dev",
    "test": "bnr test",
    "test:watch": "bnr test-watch",
    "e2e": "node config/protractor.server.js",
    "e2e:interactive": "node config/protractor.server.js --elementExplorer",
    "ionic:serve": "ionic-app-scripts serve",
    "ionic:build": "ionic-app-scripts build",
    "ios:dev": "bnr ios:dev",
    "ios:release": "bnr ios:release",
    "ios:release:ci": "bnr ios:release:ci",
    "android:dev": "bnr android:dev",
    "android:release": "bnr android:release",
    "cz": "git-cz",
    "docs": "typedoc --options typedoc.json --exclude '**/*+(e2e|spec|index).ts' ./src/",
    "release": "standard-version --no-verify",
    "push": "git push --follow-tags origin master",
    "lint": "tslint \"src/**/*.ts\" --project ./tsconfig.json",
    "scss-lint": "scss-lint",
    "outdated": "npm outdated --depth 0",
    "postinstall": "webdriver-manager update --standalone",
    "precommit": "npm run lint && npm run scss-lint",
    "prepush": "npm test",
    "commitmsg": "validate-commit-msg"
  },
  "betterScripts": {
    "dev": {
      "command": "ionic-app-scripts serve",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "web"
      }
    },
    "build": {
      "command": "ionic-app-scripts build --prod --tsconfig ./tsconfig.build.json",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-dev": {
      "command": "ionic-app-scripts build --dev",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test-watch": {
      "command": "karma start --auto-watch --no-single-run",
      "env": {
        "NODE_ENV": "test",
        "NO_COVERAGE": true
      }
    },
    "ios:dev": {
      "command": "ionic cordova run ios --livereload",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "ios"
      }
    },
    "ios:release": {
      "command": "ionic cordova build ios --prod --release",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "ios"
      }
    },
    "ios:release:ci": {
      "command": "npm run build",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "ios"
      }
    },
    "android:dev": {
      "command": "ionic cordova run android --livereload",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "android"
      }
    },
    "android:release": {
      "command": "ionic cordova build android --prod --release",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "android"
      }
    }
  },
  "dependencies": {
    "@angular/common": "5.0.0",
    "@angular/compiler": "5.0.0",
    "@angular/compiler-cli": "5.0.0",
    "@angular/core": "5.0.0",
    "@angular/forms": "5.0.0",
    "@angular/http": "5.0.0",
    "@angular/platform-browser": "5.0.0",
    "@angular/platform-browser-dynamic": "5.0.0",
    "@ionic-native/core": "4.4.2",
    "@ionic-native/splash-screen": "4.4.2",
    "@ionic/storage": "2.1.3",
    "cordova-android": "6.4.0",
    "cordova-ios": "4.5.3",
    "cordova-plugin-app-version": "0.1.9",
    "cordova-plugin-crosswalk-webview": "2.3.0",
    "cordova-plugin-device": "1.1.7",
    "cordova-plugin-ionic-keyboard": "2.0.4",
    "cordova-plugin-ionic-webview": "1.1.16",
    "cordova-plugin-splashscreen": "4.1.0",
    "cordova-plugin-statusbar": "2.3.0",
    "cordova-plugin-whitelist": "1.3.3",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "lodash-es": "4.17.4",
    "rxjs": "5.5.2",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular/language-service": "5.0.0",
    "@ionic/app-scripts": "3.1.2",
    "@types/debug": "ts2.4",
    "@types/jasmine": "ts2.4",
    "@types/lodash-es": "ts2.4",
    "@types/node": "ts2.4",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.4.0",
    "better-npm-run": "0.1.0",
    "codelyzer": "4.0.1",
    "commitizen": "2.9.6",
    "copy": "0.3.1",
    "cz-conventional-changelog": "2.1.0",
    "finalhandler": "1.1.0",
    "husky": "0.14.3",
    "istanbul-instrumenter-loader": "3.0.0",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-remap-coverage": "0.1.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.6",
    "protractor": "5.2.0",
    "protractor-jasmine2-screenshot-reporter": "0.4.1",
    "raw-loader": "0.5.1",
    "serve-static": "1.13.1",
    "source-map-loader": "0.2.3",
    "standard-version": "4.2.0",
    "ts-helpers": "1.1.2",
    "ts-node": "3.3.0",
    "tslint": "5.8.0",
    "typescript": "~2.4.2",
    "validate-commit-msg": "2.14.0",
    "webpack": "3.6.0",
    "xml2js": "0.4.19"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ionic_webpack": "./config/webpack.config.js",
    "ionic_aot_write_to_disk": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcoturi/ionic2-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/marcoturi/ionic2-boilerplate/issues"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3"
  },
  "cordova": {
    "platforms": [
      "android",
      "ios"
    ],
    "plugins": {
      "cordova-plugin-crosswalk-webview": {
        "XWALK_VERSION": "22+",
        "XWALK_LITEVERSION": "xwalk_core_library_canary:17+",
        "XWALK_COMMANDLINE": "--disable-pull-to-refresh-effect",
        "XWALK_MODE": "embedded",
        "XWALK_MULTIPLEAPK": "true"
      },
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-app-version": {},
      "ionic-plugin-keyboard": {}
    }
  }
}
