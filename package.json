{
  "name": "ionic2-boilerplate",
  "version": "0.25.0",
  "description": "An Ionic Starter kit featuring Ionic, Karma, Protractor, Jasmine, CI, and much more",
  "keywords": [
    "ionic2",
    "angular2",
    "roolup",
    "webpack",
    "yarn",
    "typescript",
    "karma",
    "protractor",
    "istanbul",
    "gitlab CI"
  ],
  "author": "Marco Turi <marco.turi@hotmail.it>",
  "homepage": "https://github.com/marcoturi/ionic2-boilerplate",
  "license": "MIT",
  "scripts": {
    "dev": "bnr dev",
    "build": "bnr build",
    "build-dev": "bnr build-dev",
    "test": "bnr test",
    "test:watch": "bnr test-watch",
    "e2e": "node config/protractor.server.js",
    "e2e:interactive": "node config/protractor.server.js --elementExplorer",
    "ionic:serve": "ionic-app-scripts serve",
    "ionic:build": "ionic-app-scripts build",
    "ios:dev": "bnr ios:dev",
    "ios:release": "bnr ios:release",
    "ios:release:ci": "bnr ios:release:ci",
    "android:dev": "bnr android:dev",
    "android:release": "bnr android:release",
    "cz": "git-cz",
    "docs": "typedoc --options typedoc.json --exclude '**/*+(e2e|spec|index).ts' ./src/",
    "release": "standard-version --no-verify",
    "push": "git push --follow-tags origin master",
    "lint": "tslint \"src/**/*.ts\"",
    "scss-lint": "scss-lint",
    "outdated": "npm outdated --depth 0",
    "post-install": "webdriver-manager update --standalone",
    "precommit": "npm run lint && npm run scss-lint",
    "prepush": "npm test",
    "commitmsg": "validate-commit-msg"
  },
  "betterScripts": {
    "dev": {
      "command": "ionic serve",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "web"
      }
    },
    "build": {
      "command": "ionic-app-scripts build --prod",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-dev": {
      "command": "ionic-app-scripts build --dev",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test-watch": {
      "command": "karma start --auto-watch --no-single-run",
      "env": {
        "NODE_ENV": "test",
        "NO_COVERAGE": true
      }
    },
    "ios:dev": {
      "command": "ionic cordova run ios --livereload",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "ios"
      }
    },
    "ios:release": {
      "command": "ionic cordova build ios --prod --release",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "ios"
      }
    },
    "ios:release:ci": {
      "command": "npm run build",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "ios"
      }
    },
    "android:dev": {
      "command": "ionic cordova run android --livereload",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "android"
      }
    },
    "android:release": {
      "command": "ionic cordova build android --prod --release",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "android"
      }
    }
  },
  "dependencies": {
    "@angular/common": "4.1.2",
    "@angular/compiler": "4.1.2",
    "@angular/compiler-cli": "4.1.2",
    "@angular/core": "4.1.2",
    "@angular/forms": "4.1.2",
    "@angular/http": "4.1.2",
    "@angular/platform-browser": "4.1.2",
    "@angular/platform-browser-dynamic": "4.1.2",
    "@angular/tsc-wrapped": "4.1.2",
    "@ionic-native/core": "3.12.1",
    "@ionic-native/splash-screen": "3.12.1",
    "@ionic/storage": "2.0.1",
    "ionic-angular": "3.3.0",
    "ionicons": "3.0.0",
    "lodash-es": "^4.17.4",
    "rxjs": "5.1.1",
    "sw-toolbox": "3.4.0",
    "zone.js": "0.8.11"
  },
  "devDependencies": {
    "@angular/language-service": "4.1.2",
    "@ionic/app-scripts": "1.3.7",
    "@ionic/cli-plugin-ionic-angular": "1.3.0",
    "@ionic/cli-plugin-cordova": "1.3.0",
    "@types/jasmine": "ts2.3",
    "@types/lodash-es": "ts2.3",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.2",
    "better-npm-run": "0.0.15",
    "codelyzer": "3.0.1",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "finalhandler": "^1.0.3",
    "husky": "^0.13.4",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "^2.6.2",
    "jasmine-spec-reporter": "^4.1.0",
    "json-loader": "^0.5.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-remap-coverage": "0.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "2.0.3",
    "protractor": "5.1.2",
    "protractor-jasmine2-screenshot-reporter": "^0.3.5",
    "serve-static": "^1.12.3",
    "source-map-loader": "^0.2.1",
    "standard-version": "^4.1.0",
    "ts-helpers": "^1.1.1",
    "ts-node": "^3.0.4",
    "tslint": "5.4.2",
    "tslint-loader": "^3.5.3",
    "typedoc": "^0.7.1",
    "typescript": "~2.3.3",
    "validate-commit-msg": "^2.12.1",
    "webdriver-manager": "^12.0.6",
    "webpack": "2.6.1",
    "xml2js": "^0.4.17"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ionic_webpack": "./config/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcoturi/ionic2-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/marcoturi/ionic2-boilerplate/issues"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3"
  }
}
